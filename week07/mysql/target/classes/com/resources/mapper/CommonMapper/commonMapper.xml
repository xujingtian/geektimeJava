<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.split.dao.CommonMapper">
    <select id="queryUserByUserId" resultType="org.split.entity.User">
        select user_id as userId, user_name as userName, phone_number as phoneNumber from t_user where user_id in (1,2,3)
    </select>
    <insert id="insert">
        insert t_user values (#{userId}, #{userName}, #{phoneNumber});
    </insert>
</mapper>